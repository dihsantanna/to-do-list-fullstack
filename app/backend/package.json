{
  "name": "to-do-list-backend",
  "version": "1.0.0",
  "author": "Diogo Sant'Anna",
  "license": "MIT",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "postinstall": "./node_modules/.bin/netlify-lambda install",
    "start": "./node_modules/.bin/netlify-lambda serve dist/src",
    "prebuild": "yarn build:dev",
    "build": "./node_modules/.bin/netlify-lambda build dist/src",
    "ts-check": "tsc --noEmit --lib ES2015 ./src/*.ts",
    "dev": "tsnd -r tsconfig-paths/register --transpile-only --respawn --ignore-watch node_modules src/server.ts",
    "build:dev": "tsc && tsc-alias",
    "lint": "eslint ./src --ext .ts --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "db:generate": "npx prisma generate",
    "db:seed": "npx prisma db seed"
  },
  "dependencies": {
    "@decorators/di": "^1.0.3",
    "@prisma/client": "^4.6.1",
    "aws-sdk": "^2.1261.0",
    "babel-loader": "^9.1.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "loader-runner": "^4.3.0",
    "mock-aws-s3": "^4.0.2",
    "netlify-lambda": "^2.0.15",
    "nock": "^13.2.9",
    "serverless-http": "^3.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "8.27.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "prisma": "^4.6.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.7.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3",
    "vitest": "^0.25.2"
  },
  "prisma": {
    "schema": "src/db/prisma/schema.prisma",
    "seed": "tsnd src/db/prisma/seed.ts"
  }
}
